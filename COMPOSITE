#include<stdio.h>
int main ()
{
    int i,n,a[100];
    printf("enter size:");
    scanf("%d",&n);
    printf("enter elements\n");
    for(i=0;i<n;i++)
    	{
        scanf("%d",&a[i]);
    
    for(i=0;i<n;i++)
       if(a[i]==2)
          printf("composite number");
      
      else
      
      	printf("not composite");
	    }
	  return 0;
}
