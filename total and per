#include<stdio.h>
int main()
{
    struct student std;
    int i;
 
    printf("Enter name: ");
    gets(std.name);
    printf("enter the roll no:");#include <stdio.h>
 
struct student
{
    char name [30];
    int  marks[ 5];
    int  total,roll no;
    float percentage;
};
 
    scanf("%d",roll no);
 
    printf("Enter marks:\n");
    std.total=0;
    for(i=0;i< 3;i++){
        printf("Marks in subject %d: ",i+1);
        scanf("%d",&std.marks[i]);
        std.total+=std.marks[i];
    }
    std.percentage=(float)((float)std.total/(float)300)*100;
 
    printf("\nName: %s \nTotal Marks: %d \nPercentage: %.2f",std.name,std.total,std.percentage);
 
