#include <stdio.h>    
     
int main()    
{    
       int arr[10],i;   
		 printf("enter num of elements in array"); 
        for(i=0;i<10;i++)
        scanf("%d",&arr[i]);
    printf("Elements of given array present on odd position: \n");    
         for (int i = 0; i <10; i++)   
        if(i%2!=0)
        {
        	printf("%d",arr[i]);
		}
      
    return 0;    
}
